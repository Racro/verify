<div class="line number26 index25 alt1"><code class="undefined spaces">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</code><code class="plain">prev.</code><code class="functions">next</code> <code class="keyword">=</code> <code class="plain">temp.</code><code class="functions">next</code></div>
<div class="line number27 index26 alt2"><code class="undefined spaces">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</code><code class="keyword">else</code><code class="plain">:</code></div>
<div class="line number28 index27 alt1"><code class="undefined spaces">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</code><code class="plain">prev </code><code class="keyword">=</code> <code class="plain">temp</code></div>
<div class="line number29 index28 alt2">&nbsp;</div>
<div class="line number30 index29 alt1"><code class="undefined spaces">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</code><code class="comments"># Position was greater than</code></div>
<div class="line number31 index30 alt2"><code class="undefined spaces">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</code><code class="comments"># number of nodes in the list</code></div>
<div class="line number32 index31 alt1"><code class="undefined spaces">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</code><code class="keyword">if</code> <code class="plain">prev </code><code class="keyword">is</code> <code class="color1">None</code><code class="plain">:</code></div>
<div class="line number33 index32 alt2"><code class="undefined spaces">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</code><code class="keyword">break</code></div>
<div class="line number34 index33 alt1"><code class="undefined spaces">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</code><code class="plain">temp </code><code class="keyword">=</code> <code class="plain">temp.</code><code class="functions">next</code></div>
<div class="line number35 index34 alt2"><code class="undefined spaces">&nbsp;&nbsp;&nbsp;&nbsp;</code><code class="keyword">return</code> <code class="plain">head</code></div>
<div class="line number36 index35 alt1">&nbsp;</div>
<div class="line number37 index36 alt2">&nbsp;</div>
<div class="line number38 index37 alt1"><code class="keyword">def</code> <code class="plain">printList(head):</code></div>
<div class="line number39 index38 alt2"><code class="undefined spaces">&nbsp;&nbsp;&nbsp;&nbsp;</code><code class="keyword">while</code><code class="plain">(head):</code></div>
<div class="line number40 index39 alt1"><code class="undefined spaces">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</code><code class="keyword">if</code> <code class="plain">head.</code><code class="functions">next</code> <code class="keyword">=</code><code class="keyword">=</code> <code class="color1">None</code><code class="plain">:</code></div>
<div class="line number41 index40 alt2"><code class="undefined spaces">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</code><code class="functions">print</code><code class="plain">(</code><code class="string">"["</code><code class="plain">, head.number, </code><code class="string">"] "</code><code class="plain">, </code><code class="string">"["</code><code class="plain">, </code><code class="functions">hex</code><code class="plain">(</code><code class="functions">id</code><code class="plain">(head)), </code><code class="string">"]-&gt;"</code><code class="plain">, </code><code class="string">"nil"</code><code class="plain">)</code></div>
<div class="line number42 index41 alt1"><code class="undefined spaces">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</code><code class="keyword">else</code><code class="plain">:</code></div>
<div class="line number43 index42 alt2"><code class="undefined spaces">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</code><code class="keyword">print</code><code class="plain">(</code><code class="string">"["</code><code class="plain">, head.number, </code><code class="string">"] "</code><code class="plain">, </code><code class="string">"["</code><code class="plain">, </code><code class="functions">hex</code><code class="plain">(</code></div>
<div class="line number44 index43 alt1"><code class="undefined spaces">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</code><code class="functions">id</code><code class="plain">(head)), </code><code class="string">"]-&gt;"</code><code class="plain">, </code><code class="functions">hex</code><code class="plain">(</code><code class="functions">id</code><code class="plain">(head.</code><code class="functions">next</code><code class="plain">)))</code></div>
<div class="line number45 index44 alt2"><code class="undefined spaces">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</code><code class="plain">head </code><code class="keyword">=</code> <code class="plain">head.</code><code class="functions">next</code></div>
<div class="line number46 index45 alt1"><code class="undefined spaces">&nbsp;&nbsp;&nbsp;&nbsp;</code><code class="keyword">print</code><code class="plain">("")</code></div>
<div class="line number47 index46 alt2"><code class="undefined spaces">&nbsp;&nbsp;&nbsp;&nbsp;</code><code class="functions">print</code><code class="plain">("")</code></div>
<div class="line number48 index47 alt1">&nbsp;</div>
<div class="line number49 index48 alt2">&nbsp;</div>
<div class="line number50 index49 alt1"><code class="plain">head </code><code class="keyword">=</code> <code class="plain">Node(</code><code class="value">0</code><code class="plain">)</code></div>
<div class="line number51 index50 alt2"><code class="plain">head </code><code class="keyword">=</code> <code class="plain">push(head, </code><code class="value">1</code><code class="plain">)</code></div>
<div class="line number52 index51 alt1"><code class="plain">head </code><code class="keyword">=</code> <code class="plain">push(head, </code><code class="value">2</code><code class="plain">)</code></div>
<div class="line number53 index52 alt2"><code class="plain">head </code><code class="keyword">=</code> <code class="plain">push(head, </code><code class="value">3</code><code class="plain">)</code></div>
<div class="line number54 index53 alt1">&nbsp;</div>
<div class="line number55 index54 alt2"><code class="plain">printList(head)</code></div>
<div class="line number56 index55 alt1">&nbsp;</div>
<div class="line number57 index56 alt2"><code class="comments"># Delete any position from list</code></div>
<div class="line number58 index57 alt1"><code class="plain">head </code><code class="keyword">=</code> <code class="plain">deleteN(head, </code><code class="value">1</code><code class="plain">)</code></div>
<div class="line number59 index58 alt2"><code class="plain">printList(head)</code></div>
<div class="line number60 index59 alt1">&nbsp;</div>
<div class="line number61 index60 alt2"><code class="comments"># This code is contributed by Yash Agawral(yashagawral2852002)</code></div>
</div>
</td>
</tr>
</tbody>
</table>
</div></div>
