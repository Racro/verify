{
    "manifest_version": 3,
    "name": "Auto Insert Div Extension",
    "version": "1.0",
    "description": "An extension to automatically insert a div element into a webpage.",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  